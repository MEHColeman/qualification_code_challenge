<p id="notice"><%= notice %></p>

<h1>Listing Qualifications</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qualifications.each do |qualification| %>
      <tr>
        <td><%= qualification.name %></td>
        <td><%= link_to 'Show', qualification %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
